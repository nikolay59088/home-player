{
  "name": "modern-home-player",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"next dev\" \"ts-node --esm .\\db\\index.ts\"",
    "db": "ts-node --esm .\\db\\index.ts",
    "build": "next build",
    "start": "next start ",
    "lint": "next lint",
    "typeorm": "typeorm-ts-node-esm",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate .\\db\\migration -d .\\db\\data-source.ts",
    "typeorm:migration:create": "npm run typeorm -- migration:create .\\db\\migration\\NewMigration",
    "typeorm:migration:run": "npm run typeorm -- migration:run"
  },
  "dependencies": {
    "@svgr/webpack": "^8.0.1",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "mysql": "^2.18.1",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.63.6",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "concurrently": "^8.2.0"
  }
}
